<keymap version="1" name="Default copy 2" parent="$default">
  <action id="$Cut">
    <keyboard-shortcut first-keystroke="ctrl x" />
  </action>
  <action id="$Redo">
    <keyboard-shortcut first-keystroke="shift ctrl z" />
  </action>
  <action id="$Undo">
    <keyboard-shortcut first-keystroke="ctrl z" />
  </action>
  <action id="ActivateDatabaseToolWindow">
    <keyboard-shortcut first-keystroke="alt d" />
  </action>
  <action id="ActivateRemoteHostToolWindow">
    <keyboard-shortcut first-keystroke="alt h" />
  </action>
  <action id="EditorActions">
    <keyboard-shortcut first-keystroke="ctrl semicolon" />
  </action>
  <action id="EditorDeleteToLineEnd">
    <keyboard-shortcut first-keystroke="shift ctrl alt delete" />
  </action>
  <action id="EditorDeleteToLineStart">
    <keyboard-shortcut first-keystroke="ctrl alt back_space" />
  </action>
  <action id="EditorToggleUseSoftWraps">
    <keyboard-shortcut first-keystroke="alt w" />
  </action>
  <action id="QuickList.Deployment">
    <keyboard-shortcut first-keystroke="alt l" />
  </action>
  <action id="ToggleFullScreen">
    <keyboard-shortcut first-keystroke="ctrl alt f11" />
  </action>
  <action id="com.jetbrains.plugins.remotesdk.console.RunSshConsoleAction">
    <keyboard-shortcut first-keystroke="alt s" />
  </action>
</keymap>